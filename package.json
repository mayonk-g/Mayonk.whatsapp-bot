{
  "name": "mayonk-bot",
  "version": "1.8.7",
  "description": "A powerful multi-functional Discord bot with AI, entertainment, moderation, and utility features",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "lint": "eslint .",
    "format": "prettier --write .",
    "build": "node build.js",
    "deploy": "node deploy-commands.js",
    "docker:build": "docker build -t mayonk-bot .",
    "docker:run": "docker run -d --name mayonk-bot --env-file .env mayonk-bot",
    "docker:logs": "docker logs -f mayonk-bot",
    "pm2:start": "pm2 start ecosystem.config.js",
    "pm2:stop": "pm2 stop mayonk-bot",
    "pm2:restart": "pm2 restart mayonk-bot",
    "pm2:logs": "pm2 logs mayonk-bot",
    "backup": "node scripts/backup.js",
    "clean": "rimraf temp/* logs/*",
    "stats": "node scripts/stats.js"
  },
  "keywords": [
    "discord",
    "bot",
    "ai",
    "moderation",
    "entertainment",
    "utility",
    "multi-purpose"
  ],
  "author": "Laurie",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/laurie/mayonk-bot.git"
  },
